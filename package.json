    "googleapis": "^118.0.0",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.536.0",
    "mongodb": "^5.9.2",
    "mongoose": "^7.6.10",
    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "next-sitemap": "^4.2.3",
    "nextjs-toploader": "^1.6.11",
    "node-fetch": "^3.3.2",
