{
  "name": "ship-fast-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/logger.js --clear \"lsof -ti :3001 | xargs kill -9 2>/dev/null || true && next dev -p 3001\"",
    "dev-no-log": "lsof -ti :3001 | xargs kill -9 2>/dev/null || true && next dev -p 3001",
    "build": "next build",
    "build-no-log": "next build",
    "postbuild": "next-sitemap",
    "start": "node scripts/logger.js \"next start -p 3001\"",
    "start-no-log": "next start -p 3001",
    "lint": "node scripts/logger.js \"next lint\"",
    "lint-no-log": "next lint",
    "kill": "lsof -ti :3001 | xargs kill -9 2>/dev/null || true",
    "generate-video-titles": "node scripts/logger.js \"tsx scripts/generate-video-titles.ts\"",
    "cleanup-video-titles": "node scripts/logger.js \"tsx scripts/cleanup-video-titles.ts\"",
    "setup-marketing-images": "tsx upload-marketing-images-to-supabase.ts",
    "stop": "lsof -ti :3001 | xargs kill -9 2>/dev/null || true",
    "clear-logs": "echo '' > log.txt && echo 'Log file cleared' || echo 'No log file to clear'",
    "view-logs": "tail -f log.txt",
    "view-recent-logs": "tail -100 log.txt",
    "rotate-logs": "node scripts/log-rotator.js",
    "rotate-logs-force": "node scripts/log-rotator.js --force",
    "log-status": "node scripts/log-rotator.js --help && echo '' && echo 'Current log size:' && ls -lh log.txt 2>/dev/null || echo 'No log file found'",
    "extract-schools": "cd school-data-extraction && npm run full-extraction",
    "extract-drive": "cd school-data-extraction && npm run extract-drive",
    "organize-schools": "cd school-data-extraction && npm run organize-schools",
    "generate-school-integration": "cd school-data-extraction && tsx scripts/website-integration-helper.ts",
    "setup-school-extraction": "cd school-data-extraction && npm install",
    "upload-student-data": "node scripts/logger.js \"tsx scripts/upload-student-data.ts\"",
    "setup-student-tables": "node scripts/logger.js \"tsx scripts/setup-student-tables.ts\"",
    "clean-school-data": "rm -rf public/schools/* && rm -rf school-data-extraction/downloads/*",
    "migrate-to-database": "tsx scripts/migrate-school-data-to-database.ts",
    "deploy-database": "supabase db push && npm run migrate-to-database"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.0",
